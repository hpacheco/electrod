(** Symbols are hash-consed strings *)

type t

val make : string -> t
val compare : t -> t -> int
val compare_string : t -> t -> int
val hash : t -> int
val equal : t -> t -> bool

include Intf.Print.S with type t := t

val to_string : t -> string
val to_yojson : t -> Yojson.Safe.t
val of_yojson : Yojson.Safe.t -> (t, string) result